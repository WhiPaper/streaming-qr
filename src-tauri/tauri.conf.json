{
  "$schema": "https://schema.tauri.app/config/2",
  "identifier": "com.example.streamingqr",
  "productName": "Streaming QR",
  "version": "0.1.0",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "Streaming QR Prototype",
        "width": 1024,
        "height": 720,
        "resizable": true
      }
    ]
  },
  "bundle": {
    "active": true
  }
}
